{
  "manifest_version": 3,
  "name": "EyeSees",
  "version": "1.2.0",
  "author": "Devin",
  "description": "Handy tool to search for posts in a niche community",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.imdb.com/title/*"
      ],
      "js": [
        "content-imdb.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "options_ui": {
    "page": "preferences.html"
  }
}