{
  "manifest_version": 2,
  "name": "EyeSees",
  "version": "1.2.0",
  "author": "Devin",
  "description": "Handy tool to search for posts in an awesome site",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.imdb.com/title/*"
      ],
      "js": [
        "content-imdb.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "menus",
    "storage"
  ],
  "options_ui": {
    "page": "preferences.html",
    "browser_style": true,
    "chrome_style": true
  },
  "browser_specific_settings": {
    "gecko": {
        "id": "eyesees@epoweri.com",
        "strict_min_version": "57.0a1"
    }
}  
}